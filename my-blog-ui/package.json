    "echarts": "^4.9.0",
    "element-ui": "^2.13.2",
    "file-saver": "^2.0.2",
    "fuse.js": "^3.4.3",
    "js-beautify": "^1.13.0",
    "js-cookie": "^2.2.1",
    "jsencrypt": "^3.0.0-rc.1",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
